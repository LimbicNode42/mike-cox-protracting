  "author": "LimbicNode42",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18"
