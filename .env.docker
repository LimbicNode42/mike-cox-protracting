# Environment file for Docker deployment
# Copy this to .env and fill in your values

# Keycloak Configuration
KEYCLOAK_URL=https://your-keycloak-instance.com
KEYCLOAK_USERNAME=your-username
KEYCLOAK_PASSWORD=your-password

# Infisical Configuration (Token auth - recommended)
INFISICAL_TOKEN=your-infisical-token
INFISICAL_ORG_ID=your-org-id
INFISICAL_PROJECT_ID=your-project-id
INFISICAL_ENVIRONMENT_SLUG=dev

# Infisical Universal Auth (fallback if no token)
INFISICAL_CLIENT_ID=your-client-id
INFISICAL_CLIENT_SECRET=your-client-secret
INFISICAL_SITE_URL=https://app.infisical.com

# Server Configuration
HOST=0.0.0.0
PORT=8000
